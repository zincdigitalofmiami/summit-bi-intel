extends layout/layout

block vars
  - title = 'Help Center Knowledge Base Page'
  - bodyClass = 'help-center-knowledge-base-page'

block container
  +BreadcrumbBase('Help Desk', 'Home', 'Knowledge Base', false, 25, true)
  section
    +BaseWrapper.text-center.rounded-2xl.px-2(class='pt-[53px] pb-[98px]')
      h1.font-semibold.text-gray-1100(class='text-[28px] leading-[34px] dark:text-gray-dark-1100 mb-[10px]') How Can We Help?
      p.leading-4.text-gray-500.mb-9(class='text-[14px] dark:text-gray-dark-500') Check out latest updates
      +BaseWrapper.rounded-2xl.search-input-shadow.flex.items-center.justify-between.flex-col.mx-auto(
        class='py-[18px] pl-[28px] pr-[19px] mb-[37px] gap-[10px] sm:flex-row lg:w-[635px]'
      )
        .flex.items-center.flex-1
          img(src='assets/images/icons/icon-search-normal.svg', alt='seacrh icon')
          input.input.w-full.bg-transparent.outline-none.h-5.text-gray-400.text-sm.leading-4(
            type='text',
            placeholder='Search for answers',
            class='focus:!outline-none placeholder:text-gray-400 dark:placeholder:text-gray-dark-400 pl-[11px]'
          )
        .flex.items-center(class='gap-x-[14px]')
          button.text-sm.leading-4.text-gray-500.py-3.rounded-lg.px-5.hidden.items-center(
            class='dark:text-gray-dark-500 bg-[#E8EDF2] dark:bg-[#313442] xl:inline-flex gap-x-[13px]'
          ) UI/UX designing
            img.cursor-pointer(src='assets/images/icons/icon-close.svg', alt='close icon')
          +ButtonBase('Search').px-6.bg-color-brands.rounded-lg(class='py-[11.5px] hover:bg-color-brands')
      .flex.mx-auto.flex-wrap.justify-center(class='max-w-[700px] gap-[5px] mb-[58px]')
        +TagBase('PHP Developer')
        +TagBase('Python Developer')
        +TagBase('Human Resource')
        +TagBase('Joomla Developer')
        +TagBase('UI UX Designer')
        +TagBase('Program Manager')
        +TagBase('Marketing Manager')
        +TagBase('SEO Manager')
        +TagBase('Graphic Designer')
      .w-full.bg-neutral(class='h-[1px] dark:bg-dark-neutral-border mb-[65px]')
